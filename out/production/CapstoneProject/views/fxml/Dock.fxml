<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="views.screen.DockScreen">
   <children>
      <TableView fx:id="docktable" layoutX="58.0" layoutY="70.0" prefHeight="275.0" prefWidth="454.0">
        <columns>
          <TableColumn fx:id="id" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="name" prefWidth="75.0" text="Name" />
            <TableColumn fx:id="address" prefWidth="75.0" text="Address" />
            <TableColumn fx:id="numberbike" prefWidth="75.0" text="Number bike" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button layoutX="6.0" layoutY="14.0" mnemonicParsing="false" onAction="#changeSceneHome" text="Home" />
      <Button layoutX="197.0" layoutY="27.0" mnemonicParsing="false" onAction="#finddockbyid" text="Find" />
      <TextField fx:id="dockid" layoutX="267.0" layoutY="27.0" />
      <Button layoutX="526.0" layoutY="70.0" mnemonicParsing="false" onAction="#detaildock" text="Detail" />
   </children>
</AnchorPane>
